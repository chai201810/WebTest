<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="MethodScan">

	<!--insert id="addMethodScan" parameterType="MethodScan"
		useGeneratedKeys="true" keyProperty="idDyMethod">
		INSERT INTO
		dy_scan_method
		(
		id_dy_class,
		method_name,
		memo,
		depends_on_methods,
		owner
		) VALUES (
		#{idDyClass},
		#{methodName},
		#{memo},
		#{dependsOnMethods},
		#{owner}
		)
	</insert-->

	<insert id="addMethodScan" parameterType="MethodScan"
		useGeneratedKeys="true" keyProperty="idSvnMethod">
		INSERT INTO ta_svn_method2
		(
			id_svn_class,
			method_name,
			memo,
			depends_on_methods,
			owner
		) SELECT
			#{idSvnClass},
			#{methodName},
			#{memo},
			#{dependsOnMethods},
			#{owner}
		FROM dual
		WHERE NOT EXISTS 
      	(
      		SELECT 1 
      		FROM ta_svn_method2
      		WHERE
      			method_name=#{methodName}
      			AND memo=#{memo}
      			AND memo IS NOT NULL
      	)
	</insert>
</mapper> 
